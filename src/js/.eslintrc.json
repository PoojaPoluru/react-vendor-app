{
    "env": {
        "es6":true,
        "browser":true
    },
    "parserOptions":{
        "ecmaFeatures":{
            "jsx":true
        },
        "sourceType":"module"
    },
    "rules": {
        "no-undef ": "warn",
        "no-const-assign":"warn",
        "react/prefer-stateless-function": 0,
        "jsx-a11y/image-has-alt": 0,
        "prefer-template": 0,
        "react/no-did-mount-set-state": 0
    }
  }
  